{
  "name": "neft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf public/{js,css,img} && rm -rf resources/views/index.blade.php && vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint",
    "dev": "rm -rf public/{js,css,img} && rm -rf resources/views/index.blade.php && vue-cli-service build --no-clean --mode development --watch",
    "watch-dev": "npm-watch dev"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "jsonwebtoken": "^8.5.1",
    "quasar": "^2.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue3-table-lite": "^1.0.5",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "npm-watch": "^0.9.0",
    "sass": "1.32.12",
    "sass-loader": "^10.1.0",
    "vue-cli-plugin-quasar": "~4.0.1",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-loader": "^15.9.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "watch": {
    "dev": {
      "patterns": [
        "resources/assets/*",
        "./"
      ],
      "ignore": "/public/*",
      "extensions": [
        "vue",
        "json",
        "js",
        "html",
        "css",
        "scss",
        "sass"
      ]
    }
  }
}
